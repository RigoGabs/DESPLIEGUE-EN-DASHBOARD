    "    elif funcion == \"Funci칩n cociente entre polinomios\":\n",
    "        return f\"y = ({parametros[0]:.4f}x + {parametros[1]:.4f}) / ({parametros[2]:.4f}x)\"\n",
    "    elif funcion == \"Funci칩n logar칤tmica\":\n",
    "        return f\"y = {parametros[0]:.4f}ln(x) + {parametros[1]:.4f}\"\n",
    "    elif funcion == \"Funci칩n lineal con producto de coeficientes\":\n",
    "        return f\"y = {parametros[0]:.4f}x + {parametros[1]:.4f}x + {parametros[2]:.4f}x\"\n",
    "    elif funcion == \"Funci칩n cuadr치tica inversa\":\n",
    "        return f\"y = 1 / ({parametros[0]:.4f}) * x쑆"\n",
    "    elif funcion == \"Funci칩n polinomial inversa\":\n",
    "        return f\"y = ({parametros[0]:.4f} / {parametros[1]:.4f}) * x + {parametros[2]:.4f}x\"\n",
    "\n",
